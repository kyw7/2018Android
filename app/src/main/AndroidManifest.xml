<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kywlater.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.RegisterActivity" />
        <activity
            android:name=".Activity.MainViewActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".launch">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".adopt_accept" />
        <activity android:name=".contact_us" />
        <activity android:name=".about_us" />
        <activity android:name=".Activity.AdoptMoreInfoActivity" />
        <activity
            android:name=".Activity.MainTestActivity"
            android:label="@string/title_activity_main_test"></activity>
    </application>

</manifest>